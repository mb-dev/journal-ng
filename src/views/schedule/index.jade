ng-include(src="'/partials/common/memory_header.html'")
.memories_index.container.index_page
  .new.row.btn-toolbar.top
    a.btn.btn-primary.col-xs-12(ng-click="syncSchedule()") Sync Schedule

  .month-nav.row.well.wells-sm
    a.col-xs-3.prev_month(ng-click='prevMonth()', href='#') &lt;&lt; Prev
    span.col-xs-6.current_month {{currentDate | amDateFormat:'MMMM, YYYY'}}
    a.col-xs-3.next_month(ng-click='nextMonth()', href='#') Next &gt;&gt;

  .main_list.list-group   
    .list-group-item.fixed-row.header.compact
      .date.col-90 Date
      .title.rest-90 Title
    a.list-group-item.fixed-row(ng-href='/schedule/{{item.id}}', ng-repeat='item in items')
      .icons.col-90
        i.fa.fa-sun-o.fa-fw.fa-sr
        span {{item.date | unixDateFormat:'MM/DD' }}

      .title.rest-90 
        {{item.title}}
        &nbsp;
        span.label.label-default(ng-repeat="category in ::[item.category1, item.category2, item.category3] track by $index")
          {{::category}}